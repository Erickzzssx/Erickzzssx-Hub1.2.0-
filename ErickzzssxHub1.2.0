-- Alternador (Toggle)
local Toggle = Tabs.Main:AddToggle("Aimbot Erickzzssx", { Title = "Aimbot Erickzzssx" })

Toggle:OnChanged(function(Value)
    -- Tenta exibir a notificação e ativar/desativar o script
    local success, err = pcall(function()
        if Value then
            -- Verifica se o StarterGui está acessível
            local StarterGui = game:GetService("StarterGui")
            if StarterGui then
                -- Notificação de ativação
                StarterGui:SetCore("SendNotification", {
                    Title = "Aimbot Erickzzssx",
                    Text = "Script Aimbot ativado!",
                    Duration = 3
                })
            else
                print("StarterGui não encontrado.")
            end
            
            -- Ativa o script se o toggle for ativado
            print("Tentando ativar o script Aimbot...")
            local aimbotScript = game:HttpGet("https://raw.githubusercontent.com/Erickzzssx/Aimbot-Erickzzssx/main/ErickzzssxAimbot")
            if aimbotScript then
                loadstring(aimbotScript)()
            else
                print("Erro ao carregar o script Aimbot.")
            end
        else
            -- Notificação de desativação
            local StarterGui = game:GetService("StarterGui")
            if StarterGui then
                StarterGui:SetCore("SendNotification", {
                    Title = "Aimbot Erickzzssx",
                    Text = "Script Aimbot desativado!",
                    Duration = 3
                })
            end
        end
    end)
    
    -- Se ocorrer algum erro, exibe a mensagem
    if not success then
        print("Erro no callback:", err)
    end
end)

-- Slider de WalkSpeed
local WalkSpeedSlider = Tabs.LocalPlayer:AddSlider("WalkSpeedSlider", {
    Title = "WalkSpeed",
    Description = "Ajuste a velocidade do jogador",
    Default = 2,
    Min = 0,
    Max = 10,
    Rounding = 1,
    Callback = function(Value)
        local player = game.Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = Value * 10 -- Multiplicado para melhor ajuste (padrão do Roblox é 16)
                print("WalkSpeed ajustado para:", humanoid.WalkSpeed)
            else
                warn("Humanoid não encontrado!")
            end
        else
            warn("Personagem não carregado!")
        end
    end
})

-- Slider de JumpPower
local JumpPowerSlider = Tabs.LocalPlayer:AddSlider("JumpPowerSlider", {
